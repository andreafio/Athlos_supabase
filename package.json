{
  "name": "athlos-supabase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint \"{src,tests}/**/*.{ts,tsx,js}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md,sql,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md,sql,yml,yaml}\"",
    "test": "vitest run --coverage",
    "test:unit": "vitest run tests/unit --coverage",
    "test:contract": "vitest run tests/contract",
    "test:e2e": "vitest run tests/e2e",
    "coverage:report": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitest/coverage-v8": "^2.1.3",
    "eslint": "^9.8.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "prettier": "^3.3.3",
    "swagger-parser": "^10.0.3",
    "typescript": "^5.5.4",
    "vitest": "^2.1.3",
    "yaml": "^2.5.0"
  }
}
